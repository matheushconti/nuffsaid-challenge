      "**/src/**/*.js",
      "**/__tests__/**"
    ],
    "testMatch": [
      "**/__tests__/**/?(*.)(test).js?(x)"
    ]
  },
  "repository": "git@gitlab.com:arnab8/nuffsaid-frontend-coding-challenge.git"
